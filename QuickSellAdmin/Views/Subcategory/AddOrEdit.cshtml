@model QuickSellAdmin.Models.SubCategory


@{
    /**/

    Layout = null;
}



@using (Html.BeginForm("AddOrEdit", "Subcategory", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.id)
    <div class="form-group">
        @Html.LabelFor(model => model.subcategoryname, new { @class = "control-label" })
        @Html.EditorFor(model => model.subcategoryname, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.subcategoryname)
    </div>

    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn" />
    </div>
}
