@model QuickSellAdmin.Models.savedAdd


@{
    /**/

    Layout = null;
}



@using (Html.BeginForm("AddOrEdit", "SavedAd", FormMethod.Post, new {onsubmit="return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.Id)
    <div class="form-group">
        @Html.LabelFor(model => model.AddID, new { @class = "control-label" })
        @Html.EditorFor(model => model.AddID, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.AddID)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.UseriD, new { @class = "control-label" })
        @Html.EditorFor(model => model.UseriD, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.UseriD)
    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn" />
    </div>
}
